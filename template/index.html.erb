<html>
  <head>
    <title>Automorphic numbers</title>
    <link rel="stylesheet" href="../template/my_style.css">
    <b>Automorphic Numbers</b>
  </head>

  <body>

    <table border>

      <tr>
        <th>Number</th>
        <th>Square</th>
      </tr>

    <% numbers.each do |number| %>

      <tr>
        <td><%=number %></td>
        <td class=square><%= number ** 2 %></td>

      </tr>



  <% end %>
  </table>

  </body>


</html>
